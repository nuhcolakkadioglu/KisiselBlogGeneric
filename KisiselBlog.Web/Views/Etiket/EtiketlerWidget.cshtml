@model IEnumerable<KisiselBlog.Domain.Entities.Etiket>
@using KisiselBlog.Domain.Entities


    <div class="sidebox widget">
        <h3 class="widget-title section-title">Tags</h3>
        <ul class="tag-list">
@foreach (Etiket item in Model)
{
            <li><a href="/Etiket/Index/@item.EtiketId">@item.Adi (@item.Makale.Count)</a></li>
}
        </ul>
        <!-- /.tag-list -->

    </div>
